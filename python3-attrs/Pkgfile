# Name: python3-attrs
# Description: classes without boilerplate
# Depends on: python3-setuptools

name=python3-attrs
version=19.1.0
release=1
source=(https://github.com/python-attrs/attrs/archive/$version.tar.gz)

build() {
	cd attrs-$version
	python3 setup.py build	
	python3 setup.py install --root $PKG
}
