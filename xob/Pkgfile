# Name: xob
# Depends: libX11 libconfig
name="xob"
version="0.1.1"
release=1
source=(https://github.com/florentc/xob/archive/v$version.tar.gz)

build() {
    cd $name-$version
    cd src
    pwd; ls
    patch -p1 < ../../../../xobrc-cleanup.diff
    cd ..
    make
    make \
        DESTDIR=$PKG PREFIX=$DESTDIR/usr/local \
        install
}


# vim:ft=sh
