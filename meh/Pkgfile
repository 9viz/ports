# Name: meh
# Description: simple image viewer
# Dependencies: libjpeg-turbo giflib libpng

name="meh"
version="69f653a1f16d11b12e5b600e808a740898f3223e"
release=1
source=(https://github.com/afarah1/meh/archive/$version.zip)

build() {
    cd $name-$version
    make; make PREFIX=/usr DESTDIR=$PKG install
}